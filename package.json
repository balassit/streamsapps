{
  "name": "streamsapps",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build --build-optimizer --aot",
    "build:prod": "ng build --build-optimizer --aot --prod",
    "test": "ng test --watch --code-coverage --browsers Chrome",
    "test:ci": "ng test --code-coverage --browsers ChromeHeadless",
    "deploy": "npx ngh",
    "lint": "ng lint",
    "pree2e": "node node_modules/protractor/bin/webdriver-manager update --gecko false --standalone false --ignore_ssl",
    "wdm:update:ci": "node node_modules/protractor/bin/webdriver-manager update --gecko false --chrome false --standalone false --ignore_ssl",
    "e2e": "ng e2e",
    "atdd:ci": "ng e2e --protractor-config atdd/conf/protractor.conf.ci.js --webdriver-update false && node format-atdd.js"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^7.0.0",
    "@angular/cdk": "^7.0.0",
    "@angular/common": "^7.0.0",
    "@angular/compiler": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@angular/forms": "^7.0.0",
    "@angular/http": "^7.0.0",
    "@angular/material": "^7.0.0",
    "@angular/platform-browser": "^7.0.0",
    "@angular/platform-browser-dynamic": "^7.0.0",
    "@angular/router": "^7.0.0",
    "@ng-bootstrap/ng-bootstrap": "^3.3.0",
    "@ngrx/effects": "6.1.0",
    "@ngrx/entity": "6.1.0",
    "@ngrx/router-store": "^6.1.0",
    "@ngrx/schematics": "6.1.0",
    "@ngrx/store": "^6.1.0",
    "@ngrx/store-devtools": "6.1.0",
    "core-js": "^2.5.4",
    "hammerjs": "^2.0.8",
    "ngrx-store-freeze": "0.2.4",
    "opencollective": "1.0.3",
    "rxjs-compat": "^6.3.3",
    "rxjs": "^6.0.0",
    "rxjs-tslint": "^0.1.5",
    "zone.js": "~0.8.26"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.10.0",
    "@angular/cli": "~7.0.2",
    "@angular/compiler-cli": "^7.0.0",
    "@angular/language-service": "^7.0.0",
    "@types/chai": "^4.1.3",
    "@types/cucumber": "^4.0.4",
    "@types/jasmine": "~2.8.6",
    "@types/jasminewd2": "~2.0.3",
    "@types/node": "~8.9.4",
    "angular-cli-ghpages": "^0.5.3",
    "chai": "^4.2.0",
    "codelyzer": "~4.2.1",
    "cucumber": "^5.0.2",
    "jasmine-core": "~2.99.1",
    "jasmine-spec-reporter": "~4.2.1",
    "karma": "~1.7.1",
    "karma-chrome-launcher": "~2.2.0",
    "karma-coverage-istanbul-reporter": "~2.0.0",
    "karma-jasmine": "~1.1.1",
    "karma-jasmine-html-reporter": "^0.2.2",
    "protractor": "^5.4.1",
    "protractor-cucumber-framework": "^6.1.1",
    "ts-node": "~5.0.1",
    "tslint": "~5.9.1",
    "typescript": "~3.1.3"
  }
}
